<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".MainActivity" >

    <ImageView
        android:id="@+id/s1"
        android:layout_width="109dp"
        android:layout_height="132dp"
        android:layout_marginTop="296dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/slot1" />

    <ImageView
        android:id="@+id/s2"
        android:layout_width="109dp"
        android:layout_height="132dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="@+id/s1"
        app:layout_constraintStart_toStartOf="@+id/s1"
        app:layout_constraintTop_toBottomOf="@+id/s1"
        app:srcCompat="@drawable/slot2" />

    <ImageView
        android:id="@+id/s3"
        android:layout_width="109dp"
        android:layout_height="132dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/s1"
        app:layout_constraintEnd_toStartOf="@+id/s1"
        app:layout_constraintTop_toTopOf="@+id/s1"
        app:layout_constraintVertical_bias="0.425"
        app:srcCompat="@drawable/slot3" />

    <ImageView
        android:id="@+id/s5"
        android:layout_width="109dp"
        android:layout_height="132dp"
        android:layout_marginStart="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/s1"
        app:layout_constraintStart_toEndOf="@+id/s1"
        app:layout_constraintTop_toTopOf="@+id/s1"
        app:layout_constraintVertical_bias="0.333"
        app:srcCompat="@drawable/slot4" />

    <ImageView
        android:id="@+id/s4"
        android:layout_width="109dp"
        android:layout_height="132dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/s2"
        app:layout_constraintEnd_toStartOf="@+id/s2"
        app:layout_constraintTop_toBottomOf="@+id/s1"
        app:srcCompat="@drawable/slot5" />

    <ImageView
        android:id="@+id/s6"
        android:layout_width="109dp"
        android:layout_height="132dp"
        android:layout_marginStart="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/s2"
        app:layout_constraintStart_toEndOf="@+id/s2"
        app:layout_constraintTop_toTopOf="@+id/s2"
        app:srcCompat="@drawable/slot2" />

    <ImageView
        android:id="@+id/imageStart"
        android:layout_width="398dp"
        android:layout_height="121dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/s2"
        app:srcCompat="@drawable/button" />

    <ImageView
        android:id="@+id/imageM"
        android:layout_width="154dp"
        android:layout_height="101dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/money" />

    <TextView
        android:id="@+id/tM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#BF291F1F"
        android:text="0"
        android:textColor="#EFECEC"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="@+id/imageM"
        app:layout_constraintHorizontal_bias="0.436"
        app:layout_constraintStart_toStartOf="@+id/imageM"
        app:layout_constraintTop_toBottomOf="@+id/imageM" />

    <TextView
        android:id="@+id/textViewWin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="#BF291F1F"
        android:text="TextView"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/s1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.771" />

</androidx.constraintlayout.widget.ConstraintLayout>